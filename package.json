{
  "name": "twilio-realtime-voice-transcription",
  "version": "1.0.0",
  "description": "Real-time voice transcription service using Twilio Media Streams and Azure Speech Services",
  "main": "app.js",
  "scripts": {
    "start": "node --env-file=local.env app.js",
    "dev": "nodemon app.js",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "health": "curl http://localhost:3000/health"
  },
  "keywords": [
    "twilio",
    "voice",
    "transcription",
    "real-time",
    "azure",
    "speech-to-text",
    "websocket",
    "nodejs",
    "express"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "twilio": "^4.19.3",
    "microsoft-cognitiveservices-speech-sdk": "^1.34.1",
    "ws": "^8.16.0",
    "multer": "^1.4.5-lts.1",
    "axios": "^1.6.7",
    "dotenv": "^16.4.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "jest": "^29.7.0",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/twilio-realtime-voice-transcription.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/twilio-realtime-voice-transcription/issues"
  },
  "homepage": "https://github.com/yourusername/twilio-realtime-voice-transcription#readme",
  "config": {
    "port": 3000
  },
  "nodemonConfig": {
    "ignore": [
      "uploads/*",
      "*.test.js"
    ],
    "env": {
      "NODE_ENV": "development"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ],
    "env": {
      "node": true,
      "es2021": true,
      "jest": true
    },
    "rules": {
      "no-console": "off",
      "semi": ["error", "always"]
    }
  }
}