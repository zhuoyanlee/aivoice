{
  "name": "twilio-realtime-voice-transcription",
  "version": "1.0.0",
  "description": "Real-time voice transcription service using Twilio Media Streams and Azure Speech Services",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "local": "node --env-file=local.env app.js",
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:production": "wrangler deploy --env production",
    "deploy:staging": "wrangler deploy --env staging",
    "build": "wrangler deploy --dry-run",
    "tail": "wrangler tail",
    "kv:create": "wrangler kv:namespace create TRANSCRIPTIONS",
    "kv:create:preview": "wrangler kv:namespace create TRANSCRIPTIONS --preview",
    "durable:create": "wrangler r2 bucket create twilio-transcriptions",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "health": "curl http://localhost:3000/health"
  },
  "keywords": [
    "twilio",
    "voice",
    "transcription",
    "real-time",
    "azure",
    "speech-to-text",
    "websocket",
    "nodejs",
    "express"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "dependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "axios": "^1.6.7",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "hono": "^4.0.0",
    "itty-router": "^4.2.1",
    "microsoft-cognitiveservices-speech-sdk": "^1.34.1",
    "multer": "^1.4.5-lts.1",
    "twilio": "^4.19.3",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.4",
    "wrangler": "^4.33.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/twilio-realtime-voice-transcription.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/twilio-realtime-voice-transcription/issues"
  },
  "homepage": "https://github.com/yourusername/twilio-realtime-voice-transcription#readme",
  "config": {
    "port": 3000
  },
  "nodemonConfig": {
    "ignore": [
      "uploads/*",
      "*.test.js"
    ],
    "env": {
      "NODE_ENV": "development"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ],
    "env": {
      "node": true,
      "es2021": true,
      "jest": true
    },
    "rules": {
      "no-console": "off",
      "semi": [
        "error",
        "always"
      ]
    }
  }
}
